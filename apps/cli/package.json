{
  "name": "cartesia-download",
  "version": "1.0.0",
  "description": "CLI tool to download audio from Cartesia TTS API",
  "bin": {
    "cartesia-download": "./dist/cli.mjs"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/cli.ts",
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@cartesia-download/core": "workspace:*",
    "@cartesia/cartesia-js": "^2.2.9",
    "gunshi": "^0.29.2",
    "neverthrow": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@typescript/native-preview": "7.0.0-dev.20260226.1",
    "tsdown": "0.21.0-beta.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
