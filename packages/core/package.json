{
  "name": "@cartesia-download/core",
  "version": "1.0.0",
  "description": "Core TTS client, pipeline, and annotation for Cartesia TTS",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.48",
    "@cartesia/cartesia-js": "^2.2.9",
    "ai": "^6.0.103",
    "neverthrow": "^8.2.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.2",
    "@typescript/native-preview": "7.0.0-dev.20260226.1",
    "tsdown": "0.21.0-beta.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
